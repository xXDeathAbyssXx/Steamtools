{
	"name": "streamtools",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev --host=0.0.0.0",
		"build": "vite build",
		"preview": "vite preview --host=0.0.0.0",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prepare": "npm run smui-theme-light && npm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui/runtime/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui/runtime/smui-dark.css -i src/theme/dark"
	},
	"devDependencies": {
		"@fontsource/fira-mono": "^4.5.10",
		"@neoconfetti/svelte": "^1.0.0",
		"@smui/card": "^7.0.0-beta.15",
		"@smui/fab": "^7.0.0-beta.15",
		"@smui/layout-grid": "^7.0.0-beta.15",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.27.6",
		"@types/cookie": "^0.5.1",
		"@types/jquery": "^3.5.27",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"catenary-curve": "^2.0.1",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"lazy-brush": "^2.0.1",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"resize-observer-polyfill": "^1.5.1",
		"smui-theme": "^7.0.0-beta.15",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"svelte-pdf": "^1.0.20",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.4.2",
		"vitest": "^0.34.0"
	},
	"type": "module"
}
